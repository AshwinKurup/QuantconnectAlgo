import string
originalText = '''
verb
uapc
club

amtx
srne
cvac

tlc
mtp
opk


codx
bpsr
bxrc
hx

igc
visl
meso
ttoo

immp
clsk
aal
ivr
'''
lowercase_alphabets = string.ascii_lowercase
uppercase_alphabets = string.ascii_uppercase
uppercaseText = originalText.upper()
stockList = uppercaseText.split("\n")

stock_set = []

for i in range(len(stockList)):
    stockList[i] = "$" + stockList[i]
text = ' '.join([str(elem) for elem in stockList])
try:
    for i in range(0, len(text)):
        if text[i] == "$": #print until you see a blank space
            stock_string = ""
            for m in range(0, 20):
                if text[i+m+1] in lowercase_alphabets or text[i+m+1] in uppercase_alphabets:
                    if text[i+m+1] != "":
                        stock_string = stock_string + text[i + m + 1].upper() # disregard the "$" sign
                    else:
                        stock_set.append(stock_string)
                        break
                else:
                    stock_set.append(stock_string)
                    break
except:
    pass

try:
    while True:
        stock_set.remove('')
except ValueError:
    pass

print(stock_set)
